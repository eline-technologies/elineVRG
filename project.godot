; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="elineVRG"
run/main_scene="res://SystemWorld.tscn"
run/low_processor_mode=true
boot_splash/image="res://eline Technologies.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://eline Technologies.png"
config/windows_native_icon="res://eline.ico"

[display]

window/size/width=1454
window/size/height=776
window/dpi/allow_hidpi=true

[gdnative]

singletons=[ "res://addons/godot-openvr/godot_openvr.gdnlib", "res://addons/godot_ovrmobile/godot_ovrmobile.gdnlib" ]

[layer_names]

3d_physics/layer_1="Player"
3d_physics/layer_2="Environment"
3d_physics/layer_3="Objects"
3d_physics/layer_4="Pointer"

[rendering]

quality/filters/anisotropic_filter_level=16
quality/shadows/filter_mode=2
quality/filters/msaa=4
environment/default_environment="res://default_env.tres"
